{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Invariant": {
    "DefaultDatabase": "System.Data.SqlClient"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=tcp:{your_db_server}.database.windows.net,1433;Initial Catalog={your_db};Persist Security Info=False;User ID={user_id};Password={your_password};MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;"
  }
}
